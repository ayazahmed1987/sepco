# =========================================
# Laravel 12 - Safe & Correct Git Ignore
# =========================================

# Composer dependencies
/vendor/

# Node dependencies
/node_modules/

# Laravel storage (but keep folder structure)
storage/*.key
storage/app/public/*
storage/framework/cache/data/*
storage/framework/sessions/*
storage/framework/views/*
storage/logs/*
storage/framework/testing/*

# Laravel bootstrap cache (but keep folder)
bootstrap/cache/*.php

# Environment files
.env
.env.*
.env.backup
.env.local

# Public build outputs (everything else should stay!)
public/build/
public/hot
public/storage
public/mix-manifest.json
public/js/*.map
public/css/*.map

# Logs & debug files
*.log
*.cache
php_errors.log

# IDE / Editor files
.idea/
.vscode/
*.sublime-project
*.sublime-workspace

# OS junk
.DS_Store
Thumbs.db

# PHPUnit cache
.phpunit.result.cache

# DB backups
*.sql
*.sqlite

# Archives / large files
*.zip
*.rar
*.7z
*.tar
*.gz

# Local tools
homestead.yaml
Homestead.json

# Misc temp files
*.swp
*.bak
*.tmp

# Laravel Debugbar
/debugbar
