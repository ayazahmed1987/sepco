# Normalize line endings
* text=auto

# Treat these as binary
*.jpg binary
*.jpeg binary
*.png binary
*.gif binary
*.svg binary
*.ico binary
*.zip binary
*.gz binary
*.7z binary
*.pdf binary

# Don't export ignored files
/.env  export-ignore
/.env.example export-ignore
/node_modules export-ignore
/vendor export-ignore
/storage export-ignore
